{% extends 'base.html.twig' %}

{% block title %}LineOrder index{% endblock %}

{% block body %}
    <h1>LineOrder index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for line_order in line_orders %}
            <tr>
                <td>{{ line_order.id }}</td>
                <td>{{ line_order.quantity }}</td>
                <td>
                    <a href="{{ path('line_order_show', {'id': line_order.id}) }}">show</a>
                    <a href="{{ path('line_order_edit', {'id': line_order.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('line_order_new') }}">Create new</a>
{% endblock %}
